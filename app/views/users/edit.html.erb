<h1 class="text-center">EDIT YOUR INFO</h1>

<div class="text-center form-container">
  <%= bootstrap_form_for @user do |f| %>
    <%= f.text_field :username, label: 'Username', :class => 'better-form' %>
    <%= f.password_field :password, label: 'Password', :class => 'better-form' %>
    <%= f.submit %>
  <% end %>
</div>
